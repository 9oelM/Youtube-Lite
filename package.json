{
  "name": "controlled-youtube",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "ajv": "^6.5.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "react-youtube": "^7.6.0",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sass": "node-sass-chokidar ./src/styles/source/master.sass -o ./src/styles/output --watch --output-style \"compressed\"",
    "prettier": "onchange '**/*.js' -- prettier --write {{changed}}",
    "watch": "npm-run-all --parallel sass prettier",
    "start-and-watch": "npm-run-all --parallel sass start prettier"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3"
  }
}
